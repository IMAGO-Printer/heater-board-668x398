(version 1)

(rule "Track to Dummy Pad Clearance"
  (constraint clearance (min 0.3mm))
  (condition "A.Type == 'Track' && B.Type == 'Pad'"))

(rule "Track to Keepout Line"
  (constraint clearance (min 1mm))
  (condition "A.Type == 'Track' && B.Type == 'Keepout'"))


(rule "Track Width 0.54mm"
  (constraint track_width (max 0.54mm))
  (condition "A.Type == 'Track'"))

